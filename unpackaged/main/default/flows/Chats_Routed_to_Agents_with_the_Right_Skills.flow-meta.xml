    <assignments>
        <description>This example node saves the value of the Subject field in the pre-chat list to a variable in the Flow.</description>
        <name>CaptureSubject</name>
        <label>Capture Subject</label>
        <locationX>1065</locationX>
        <locationY>200</locationY>
        <assignmentItems>
            <assignToReference>FirstName</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>PrechatLoop.Parent.FirstName__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>LastName</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>PrechatLoop.Parent.LastName__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
