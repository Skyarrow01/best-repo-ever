<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Owned_by_Queue__c</fullName>
    <description>Set to TRUE if Case is owned by a Queue. Used for process builder flow.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Owner:Queue.QueueName)),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Owned by Queue</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
