<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Skill_Level</fullName>
    <active>true</active>
    <errorConditionFormula>(SkillLevel&gt;5||
 SkillLevel&lt;1||
ISBLANK(SkillLevel))&amp;&amp;
 $User.ProfileId &lt;&gt;&#39;00e6A000000zAlA&#39;</errorConditionFormula>
